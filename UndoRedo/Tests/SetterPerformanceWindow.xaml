<mahapps:MetroWindow x:Class="UndoRedo.Tests.SetterPerformanceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:mahapps="http://metro.mahapps.com/winfx/xaml/controls"
        mc:Ignorable="d"
        Title="SetterPerformanceView" Height="450" Width="800">
    <DockPanel>
        <DockPanel DockPanel.Dock="Top">
            <TextBox Text="{Binding TryNumber}" DockPanel.Dock="Right" Width="120" VerticalAlignment="Center" TextAlignment="Right"/>
            <Button DockPanel.Dock="Top" Content="Run" Command="{Binding RunCommand}"/>
        </DockPanel>
        <TextBox IsReadOnly="True" x:Name="analyseText"/>
    </DockPanel>
</mahapps:MetroWindow>
